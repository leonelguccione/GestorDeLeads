<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="modelo.Gestor" table="GESTOR">
    <id name="id_gestor" type="java.lang.Long">
      <column name="ID_GESTOR"/>
      <generator class="assigned"/>
    </id>
    
    <bag name="terceros" table="TERCERO" cascade = "all" inverse="false" lazy="true">
      <key>
        <column name="ID_GESTOR"/>
      </key>
      <one-to-many class="modelo.Tercero"/>
    </bag>

    <bag name="empleados" table="EMPLEADO" cascade = "all" inverse="false" lazy="true">
      <key>
        <column name="ID_GESTOR"/>
      </key>
      <one-to-many class="modelo.Empleado"/>
    </bag>
   
  </class>
</hibernate-mapping>
